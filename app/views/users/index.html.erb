<% provide(:title, 'All users') %>
<h1>All users</h1>
<%= search_form_for @q do |f| %>
  <%= f.label "Find User" %>
  <%= f.search_field :name_cont, placeholder: "Search for User" %>
  <%= f.submit %>
<% end %>
<%= will_paginate %> 
<ul class="users">
   <%= render @users %>
</ul>
<%= will_paginate %>
